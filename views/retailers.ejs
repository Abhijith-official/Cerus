      <% include ./partials/notificationbar %>

      <div class="row align-items-center justify-content-center">
        <!-- START COMING SOON-->
        <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 probootstrap-animate" data-animate-effect="fadeIn">
